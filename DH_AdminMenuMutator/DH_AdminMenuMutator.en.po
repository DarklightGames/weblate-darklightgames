msgid "DHAdminMenuMutator.FriendlyName"
msgstr "Admin menu"

msgid "DHAdminMenuMutator.Description"
msgstr ""
"Screen menu options allowing an admin greater control over players, a "
"realism match or during testing"

msgid "DHAdminMenu_AdminMessages.EnterNewPlayerName"
msgstr "Please enter a new name for the player - then press enter"

msgid "DHAdminMenu_AdminMessages.EnterPrivateMessage"
msgstr "Type your private message to the player - then press enter"

msgid "DHAdminMenu_AdminMessages.EnterWarningMessage"
msgstr "Type your warning message to the player - then press enter"

msgid "DHAdminMenu_AdminMessages.EnterKickReason"
msgstr "Type your kick message to the player - then press enter to KICK them"

msgid "DHAdminMenu_AdminMessages.ConfirmKillPlayer"
msgstr "Press enter to confirm you want to KILL this player"

msgid "DHAdminMenu_AdminMessages.ConfirmGagPlayer"
msgstr "Press enter to confirm you want to GAG this player"

msgid "DHAdminMenu_AdminMessages.ConfirmSwitchToAlliesRole"
msgstr ""
"Press enter to confirm you want to SWITCH this player to a new ALLIES role"

msgid "DHAdminMenu_AdminMessages.ConfirmSwitchToAxisRole"
msgstr ""
"Press enter to confirm you want to SWITCH this player to a new AXIS role"

msgid "DHAdminMenu_AdminMessages.ConfirmDropAtObjective"
msgstr ""
"Press enter to confirm you want to PARADROP this player at the chosen "
"objective"

msgid "DHAdminMenu_AdminMessages.EnterGridLocation"
msgstr ""
"Please specify a grid location (example: for grid E2 keypad 5, enter 'e25')"

msgid "DHAdminMenu_AdminMessages.ConfirmDropAtCurrentLocation"
msgstr ""
"Press enter to confirm you want to PARADROP this player at their CURRENT "
"location"

msgid "DHAdminMenu_AdminMessages.ConfirmEnableRealism"
msgstr "Press enter to confirm you want to ENABLE realism mode"

msgid "DHAdminMenu_AdminMessages.ConfirmDisableRealism"
msgstr "Press enter to confirm you want to DISABLE realism mode"

msgid "DHAdminMenu_AdminMessages.ConfirmMatchLive"
msgstr "Press enter to confirm you want to force realism match LIVE"

