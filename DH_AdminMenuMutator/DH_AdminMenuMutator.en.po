msgstr "ERROR: invalid map grid co-ordinates"

msgid "DHAdminMenu_ErrorMessages.ErrorMessage[23]"
msgstr ""
"To auto-login you must set up your DarkestHour.ini file with your admin "
"login details"

msgid "DHAdminMenu_ErrorMessages.ErrorMessage[24]"
msgstr "No player found in sights"

msgid "DHAdminMenu_ErrorMessages.ErrorMessage[25]"
msgstr ""
"This game mode does not allow rally points so you cannot use this option"

msgid "DHAdminMenu_ErrorMessages.ErrorMessage[26]"
msgstr "Rally point placement is already disabled"

msgid "DHAdminMenu_ErrorMessages.ErrorMessage[27]"
msgstr "Rally point placement is already enabled"

