msgstr ""
"Press enter to confirm you want to drop ALL AXIS at the chosen objective"

msgid "DHAdminMenu_AdminMessages.ConfirmDropAllAtObjective"
msgstr ""
"Press enter to confirm you want to drop ALL PLAYERS at the chosen objective"

msgid "DHAdminMenu_AdminMessages.ConfirmDisableMines"
msgstr "Press enter to confirm you want to DISABLE all minefields"

msgid "DHAdminMenu_AdminMessages.ConfirmEnableMines"
msgstr "Press enter to confirm you want to RE-ENABLE all minefields"

